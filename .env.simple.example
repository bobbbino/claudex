# Security (required for production - generate with: openssl rand -hex 32)
SECRET_KEY=claudex_default_secret_key_for_development_only

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=claudex

# Ports (change if needed)
BACKEND_PORT=8080
FRONTEND_PORT=3000
POSTGRES_PORT=5432
REDIS_PORT=6379

# Docker sandbox image
DOCKER_IMAGE=ghcr.io/mng-dev-ai/claudex-sandbox:latest

# Celery workers
CELERY_CONCURRENCY=25
CELERY_WORKER_REPLICAS=8

# Logging
LOG_LEVEL=INFO
